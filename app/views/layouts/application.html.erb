<!DOCTYPE html>
<html ng-app="app" class="<%= controller_name %> <%= action_name %>">
  <head>
    <title>Webregator</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body class="<%= controller_name %> <%= action_name %>">
    <%= yield %>
  </body>
</html>
